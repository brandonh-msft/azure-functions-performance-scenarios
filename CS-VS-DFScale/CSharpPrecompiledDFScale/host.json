{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    },
    "logLevel": {
      "default": "Trace"
    }
  },
  "extensions": {
    "durableTask": {
      "extendedSessionIdleTimeoutInSeconds": 30,
      //"HubName": "V2FunctionsLoadTest",
      "extendedSessionsEnabled": true,
      "maxConcurrentActivityFunctions": 1000,
      "maxConcurrentOrchestratorFunctions": 500
    }
  }
}